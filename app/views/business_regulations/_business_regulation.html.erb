<div id="<%= dom_id business_regulation %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">Title:</strong>
    <%= business_regulation.title %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Subtitle:</strong>
    <%= business_regulation.subtitle %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Page num:</strong>
    <%= business_regulation.page_num %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Images:</strong>
    <% business_regulation.images.each do |image| %>
      <div><%= link_to image.filename, image %></div>
    <% end %>
  </p>

</div>
